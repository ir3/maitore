<!DOCTYPE html>
<html>
<head>
  <title>Maitore</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='header'>
    <%= render :partial => 'layouts/header' -%>
  </div>
  <div id='wrapper'>
    <div id='menu'>
      <%= render :partial => 'layouts/menu' -%>
    </div>
    <div id='contents'>
      <%= yield %>
    </div>
    <div id='clear'></div>
  </div>
  <div id='footer'>
    <%= render :partial => 'layouts/footer' -%>
  </div>

</body>
</html>
